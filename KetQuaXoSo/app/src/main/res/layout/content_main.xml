<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.ngocthat.ketquaxoso.MainActivity"
    tools:showIn="@layout/app_bar_main">
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:id="@+id/linearLayout"

        >
        <ScrollView
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/scrollView3"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:orientation="vertical"
                android:visibility="visible"
                android:layout_marginBottom="10dp">

        <FrameLayout
            android:id="@+id/fragmentData"
            android:name="com.example.ngocthat.ketquaxoso.SelectProvince"
            android:layout_width="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_height="145dp"
            />
        <FrameLayout
            android:id="@+id/fragmentResult"
            android:name="com.example.ngocthat.ketquaxoso.SelectProvince"
            android:layout_width="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/fragmentData"
            />
            </RelativeLayout>>
    </ScrollView>
</RelativeLayout>
</android.support.constraint.ConstraintLayout>
